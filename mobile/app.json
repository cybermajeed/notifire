      },
      "package": "com.cyber_majeed.Notifire"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6a9a0921-daaa-4b4d-a43e-484daa7660d0"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/notify.wav"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "342d0be2-df54-4124-85a6-b3f971503821"
      }
    }
  }
}
